<div class="row">
  <div class="col-md-1"></div>
  <div class="col-md-10">
    <div id="postlist">
      <div class="panel">
        <div class="panel-heading">
          <div class="text-center">
            <div class="row">
              <div class="col-sm-9">
                <h3 class="pull-left"><%= @course.nombre %></h3>
                <h5 class="pull-left"><%= @course.puntos %></h5>
              </div>
            </div>
          </div>
        </div>

        <div class="panel-body"><%= @course.contenido %></div>

        <%= render partial: 'shared/show', locals: { header: 'Attachments', attachments: @course.attachments } %>
      </div>
    </div>
  </div>
  <div class="col-md-1"></div>
</div>

<%= link_to t(:back), :back %>
