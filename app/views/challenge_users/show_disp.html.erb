<table>
  <tr>
    <th>RetoId</th>
    <th>RetoDescripcion</th>
    <th></th>
  </tr>

  <% @challengesdisp.each do |challenge| %>
    <tr>
      <td><%= challenge.id %></td>
      <td><%= challenge.descripcion %></td>
      <td><%= link_to "Enroll", {:controller => "challenge_users", :action => "enroll", :challengeid => challenge.id, :userid => current_user.id} %></td>
    </tr>
  <% end %>

</table>


